main {
    array int[3] v1, v2;

    function int productoEscalar(int i){
        int res;
        res = 0;
        while((i>=0)){
            res = res + v1[i]*v2[i];
            i = i-1;
        }
        
        return res;
    }

    v1[0] = 0;
    v1[1] = 1;
    v1[2] = 2;
    
    v2[0] = 0;
    v2[1] = 3;
    v2[2] = 2;

    printf productoEscalar(2);

}
